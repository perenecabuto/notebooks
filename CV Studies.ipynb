    "%matplotlib inline\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "sigma = np.random.randint(5)\n",
    "noise = np.random.randn(1, 1000) * sigma\n",
    "(x, n) = np.histogram(noise, np.linspace(-10, 10, 100))\n",
    "_ = plt.plot(x)\n",
    "plt.imshow([x, n])\n"
   ]
